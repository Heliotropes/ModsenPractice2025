{
    "compilerOptions": {
      "target": "ES6", 
      "module": "ES6",
      "outDir": "./dist", 
      "rootDir": "./src", 
      "strict": true, 
      "esModuleInterop": true, 
      "resolveJsonModule": true,
      "moduleResolution": "node", 
      "skipLibCheck": true, 
      "forceConsistentCasingInFileNames": true 
    },
    "include": ["src/**/*"]
  }